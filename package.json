{
  "name": "potsdam-bot",
  "version": "0.1.34",
  "description": "This bot exist to organize a meeting in the Bürgerbüro. It is hard to get a slot, so this bot should register one automatically if one is available. It will inform you via SMS if a slot it booked.",
  "bin": {
    "potsdam-bot": "infra/bin/potsdam-bot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cabcookie/potsdam-bot.git"
  },
  "keywords": [
    "government",
    "non-digitalized",
    "AWS",
    "CDK"
  ],
  "author": "Carsten Koch",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cabcookie/potsdam-bot/issues"
  },
  "homepage": "https://github.com/cabcookie/potsdam-bot#readme",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "cdk": "cdk",
    "push": "git push --follow-tags origin main",
    "release": "standard-version",
    "major-release": "standard-version --release-as major",
    "publish": "npm run release && npm run push"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.92",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.8.0",
    "esbuild": "^0.14.13",
    "jest": "^26.4.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "aws-cdk-lib": "2.8.0",
    "aws-sdk": "^2.1063.0",
    "constructs": "^10.0.0",
    "puppeteer": "^13.1.2",
    "source-map-support": "^0.5.16"
  }
}
